{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"], 
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true, 
        "trailingComma": "all"
      }
    ],
      "prefer-arrow-callback": 0,
      "linebreak-style": 0,
      "no-use-before-define":0, 
      "func-names":0, 
      "indent": [2, "tab"], 
      "no-tabs":0,

      "handle-callback-err": 1,
      
      "array-bracket-newline": 0,
      "array-element-newline": 0,
      "complexity": 2,
      "eqeqeq": [2, "allow-null"],
      "func-name-matching": 0,
      "id-length": [2, { "min": 1, "max": 30 }],
      "max-lines": [2, 700],
      "max-params": [2, 4],
      "max-statements": [2, 24],
      "max-statements-per-line": [2, { "max": 2 }],
      "no-magic-numbers": 0,
      "new-cap": 0,
      "no-extra-parens": 1,
      "sort-keys": 0
  }
}